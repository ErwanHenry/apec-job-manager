{
  "name": "apec-job-manager",
  "version": "1.0.0",
  "description": "Outil d'automatisation pour la gestion d'annonces APEC.FR",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "dashboard": "cd dashboard && npm run dev",
    "dev:all": "concurrently \"npm run dev\" \"npm run dashboard\"",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "docker:build": "docker build -t apec-job-manager .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "health": "curl http://localhost:3000/api/health"
  },
  "keywords": [
    "apec",
    "job-automation",
    "recruitment",
    "dashboard",
    "reporting"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "puppeteer": "^21.6.1",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "joi": "^17.11.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "winston": "^3.11.0",
    "node-cron": "^3.0.3",
    "pg": "^8.11.3",
    "prisma": "^5.7.0",
    "@prisma/client": "^5.7.0",
    "axios": "^1.6.2",
    "rate-limiter-flexible": "^3.0.6",
    "compression": "^1.7.4",
    "express-validator": "^7.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
